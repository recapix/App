{
  "name": "app",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "e2e": "node config/protractor.server.js",
    "e2e:interactive": "node config/protractor.server.js --elementExplorer",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:build": "ionic-app-scripts build",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "ios:release:ci": "bnr ios:release:ci",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "cz": "git-cz",
    "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
    "release": "standard-version --no-verify",
    "push": "git push --follow-tags origin master",
    "lint": "tslint \"src/**/*.ts\"",
    "scss-lint": "scss-lint",
    "outdated": "npm outdated --depth 0",
    "post-install": "webdriver-manager update",
    "commitmsg": "validate-commit-msg",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 90 --functions 100 --lines 100"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic serve --lab",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "ios:dev": {
      "command": "ionic run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "ionic build ios --prod",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "ios:release:ci": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic build android --prod",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/platform-server": "^4.3.4",
    "@angular/tsc-wrapped": "^4.3.4",
    "@ionic-native/core": "3.12.1",
    "@ionic-native/facebook": "^4.1.0",
    "@ionic-native/google-plus": "^4.1.0",
    "@ionic-native/native-storage": "^4.1.0",
    "@ionic-native/splash-screen": "3.12.1",
    "@ionic-native/status-bar": "3.12.1",
    "@ionic/storage": "2.0.1",
    "angularfire2": "^4.0.0-rc.2",
    "bourbon": "^4.3.4",
    "cordova-android": "^6.2.3",
    "cordova-plugin-console": "^1.0.5",
    "cordova-plugin-device": "^1.1.4",
    "cordova-plugin-googleplus": "^5.1.1",
    "cordova-plugin-nativestorage": "^2.2.2",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-statusbar": "^2.2.2",
    "cordova-plugin-whitelist": "^1.3.1",
    "firebase": "^4.2.0",
    "ionic-angular": "3.6.0",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "lodash-es": "^4.17.4",
    "protractor-jasmine2-screenshot-reporter": "^0.4.0",
    "rxjs": "5.4.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@ionic/app-scripts": "2.1.3",
    "@ionic/cli-plugin-cordova": "1.6.2",
    "@ionic/cli-plugin-ionic-angular": "1.4.1",
    "@ionic/cli-plugin-proxy": "1.4.1",
    "@types/jasmine": "^2.5.53",
    "@types/lodash-es": "^4.14.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.2",
    "better-npm-run": "^0.1.0",
    "codelyzer": "^3.1.2",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.4",
    "cz-conventional-changelog": "^2.0.0",
    "finalhandler": "^1.0.4",
    "husky": "^0.14.3",
    "ionic": "3.8.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.7.0",
    "jasmine-spec-reporter": "^4.2.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "serve-static": "^1.12.4",
    "source-map-loader": "^0.2.1",
    "standard-version": "^4.2.0",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.8.0",
    "typescript": "2.3.4",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.5.4",
    "xml2js": "^0.4.17"
  },
  "description": "An Ionic project",
  "config": {
    "ionic_sass": "./config/sass.config.js",
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ionic_webpack": "./config/webpack.config.js"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-nativestorage": {},
      "cordova-plugin-googleplus": {
        "REVERSED_CLIENT_ID": "myreversedclientid"
      },
      "cordova-plugin-console": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "ionic-plugin-keyboard": {}
    },
    "platforms": [
      "android"
    ]
  }
}
